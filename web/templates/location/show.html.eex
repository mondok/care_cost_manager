<h2>Show location</h2>

<ul>

  <li>
    <strong>Building:</strong>
    <%= @location.building %>
  </li>

  <li>
    <strong>Domain:</strong>
    <%= @location.domain %>
  </li>

  <li>
    <strong>Wing unit:</strong>
    <%= @location.wing_unit %>
  </li>

  <li>
